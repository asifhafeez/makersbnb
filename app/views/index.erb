  <p>
    Please enter the details of the space available to rent below
  </p>

  <form action="/" class="navbar-form navbar-left" method="post">
    <div>
      <label for="user_name" class="form_data">User name:</label>
      <input type="text" name="user_name" />
    </div>
    <div>
      <label for="user_email" class="form_data">User email:</label>
      <input type="email" name="user_email" />
    </div>
    <div>
      <label for="space_name" class="form_data">Space name:</label>
      <input type="text" name="space_name" />
    </div>
    <div>
      <label for="price" class="form_data">Price:</label>
      <input type="text" name="price" />
    </div>
    <div>
      <label for="space_description" class="form_data">Description:</label>
      <textarea name="space_description"></textarea>
    </div>
    <div>
      <label for="available_from" class="form_data">Available from:</label>
      <input type="text" name="available_from" />
    </div>
    <div>
      <label for="available_to" class="form_data">To:</label>
      <input type="text" name="available_to" />
    </div>

    <div class="button">
      <button type="submit">Add Space</button>
    </div>
  </form>

  <script src="js/jquery.js"></script>
  <script src="js/bootstrap.js"></script>

